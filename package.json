{
    "name": "increment-and-run",
    "main": "./lib/increment-and-run",
    "version": "0.1.0",
    "description": "Make incremental changes to numeric values in your code during rapid prototyping.",
    "keywords": [],
    "activationCommands": {
        "atom-workspace": "increment-and-run:minus",
        "atom-workspace": "increment-and-run:plus",
        "atom-workspace": "increment-and-run:toggle"
    },
    "repository": "https://github.com/onewhaleid/increment-and-run",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {},
    "configSchema": {
        "atomCommands": {
            "type": "string",
            "default": "none, core:save",
            "title": "Atom commands",
            "description": "Functions to call after incrementing values. Restart Atom after making changes."
        }
    }
}
